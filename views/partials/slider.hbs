<div id="carouselExampleDark" class="carousel carousel-fade carousel-light slide" data-bs-ride="carousel">
  <div class="carousel-indicators">

       {{#each slides}}
    <button type="button" data-bs-target="#carouselExampleDark" data-bs-slide-to="{{@index}}" class="{{this.class}}" aria-current="true" aria-label="Slide 1"></button>
  {{/each}}

    {{!-- <button type="button" data-bs-target="#carouselExampleDark" data-bs-slide-to="1" aria-label="Slide 2"></button>
    <button type="button" data-bs-target="#carouselExampleDark" data-bs-slide-to="2" aria-label="Slide 3"></button>
    <button type="button" data-bs-target="#carouselExampleDark" data-bs-slide-to="3" aria-label="Slide 3"></button> --}}
  </div>

       {{!-- {{slides}} --}}
  <div class="carousel-inner">
  {{#each slides}}
    <div class="carousel-item {{this.class}}" data-bs-interval="10000">
      <img src="{{this.imageUrl}}" class="d-block w-100 slider-image" alt="...">
      <div class="carousel-caption d-none d-md-block">
        <h4>{{this.title}}</h4>
        <p>{{this.subTitle}}</p>
      </div>
    </div>
    {{/each}}


