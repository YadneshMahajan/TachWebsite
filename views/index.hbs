<section id="contact_us_section" class="py-5 background1" >
  <div class="container">
    <h1  id="con" class="text-center text-secondary ">Contact Us</h1>
    <div class="row form_section">
      <div class="col-md-12">


<form  action="/process-contact-form" method="post">

  <div class="mb-3">
    <label for="exampleInputEmail1" class="form-label">Your email address</label>
    <input name="email" type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
    <div id="emailHelp" class="form-text">We'll never share your email with anyone else.</div>
  </div>


  <div class="mb-3">
    <label for="contactNumberField" class="form-label">Your Contact number</label>
    <input name="phone"type= "number" class="form-control" id="contactNumberField">
  </div>

  <div class="form-floating mt-2">
    <div>Enter your query here</div>
  <textarea   name="query" style="height:250px;" class="form-control" placeholder="Enter your query here" id="queryField" style="height: 100px"></textarea>
  {{!-- <label for="queryField">Comments</label> --}}
</div>

  <div class="container text-center mt-5">
    <button type="submit" class="btn btn-primary">Submit</button>
  </div>
  
</form>


